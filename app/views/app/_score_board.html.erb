<h2 class="aligncenter" style="margin-top: 5%">排行榜</h2>
<% i = 1 %>
<div class="ranking">
	<table class="ranking_table" align="center" style="background-color: #E4E4E4; border: 1px solid black">
		<% @user.each do |f| %>
			<tr style="background-color: #E4E4E4;">
				<td>第<%= i %>名: </td>
				<td class="aligncenter"><%= f.name %></td>
				<td class="alignright"><%= f.score %>分</td>
			</tr>
			<% i = i + 1 %>
		<% end %>
	</table>
</div>